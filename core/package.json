{
	"author": "curt.elsasser@gmail.com",
	"dependencies": {
		"commander": "8.0.0",
		"fs-extra": "10.0.0",
		"get-stdin": "8.0.0"
	},
	"description": "Tiny's core module. He's a working man.",
	"devDependencies": {
		"@types/fs-extra": "9.0.11",
		"@types/jest": "26.0.23",
		"@types/node": "14.14.31",
		"@typescript-eslint/eslint-plugin": "4.25.0",
		"@typescript-eslint/parser": "4.25.0",
		"eslint": "7.27.0",
		"jest": "27.0.3",
		"jest-junit": "12.1.0",
		"prettier": "2.3.0",
		"ts-jest": "27.0.1",
		"typescript": "4.3.2"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"license": "ISC",
	"main": "dist/src/index.js",
	"name": "tiny-core-module",
	"repository": {
		"type": "git",
		"url": "https://github.com/celsasser/tiny-core-module.git"
	},
	"scripts": {
		"analyze": "npm run format && npm run lint:fix && npm run test",
		"build": "$(npm bin)/tsc -p tsconfig.json",
		"coverage": "jest --coverage ./test/unit",
		"format": "$(npm bin)/prettier --write './*.(js|json)'  './(src|test)/**/*.(js|json|ts)'",
		"lint": "$(npm bin)/eslint .",
		"lint:fix": "$(npm bin)/eslint --fix .",
		"rebuild": "rm -rf ./dist; rm -f ./.buildcache; npm run build",
		"test": "$(npm bin)/jest ./test/unit"
	},
	"types": "dist/src/index.d.ts",
	"version": "0.0.1"
